{
  "name": "neex",
  "version": "0.7.45",
  "description": "Neex - Modern Fullstack Framework Built on Express and Next.js. Fast to Start, Easy to Build, Ready to Deploy",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "neex": "./bin/neex"
  },
  "files": [
    "dist",
    "bin",
    "native"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/bin/neex.js",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:dev": "node ./dist/bin/neex.js px \"echo Starting frontend\" \"echo Starting backend\"",
    "test:parallel": "node ./dist/bin/neex.js parallel \"echo Building frontend\" \"echo Building backend\"",
    "test:sequence": "node ./dist/bin/neex.js run \"echo Step 1\" \"echo Step 2\" \"echo Step 3\""
  },
  "keywords": [
    "fullstack-framework",
    "nextjs",
    "express",
    "monorepo",
    "polyrepo",
    "typescript",
    "prisma",
    "cli",
    "build-system",
    "web-development",
    "node",
    "javascript",
    "production-ready",
    "dotenv",
    "parallel-execution",
    "sequential-execution"
  ],
  "author": "foshati",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.6.2",
    "chokidar": "^4.0.3",
    "commander": "14.0.2",
    "figlet": "^1.9.3",
    "figures": "^6.1.0",
    "gradient-string": "^3.0.0",
    "lodash": "^4.17.21",
    "npm-run-path": "^6.0.0",
    "p-map": "^7.0.4",
    "string-width": "^7.2.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "@types/figlet": "^1.7.0",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.14.10",
    "eslint": "^9.38.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Neexjs"
  }
}
