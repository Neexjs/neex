{
  "name": "create-neex",
  "version": "0.1.33",
  "description": "Polyrepo architecture in a single monorepo",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "create-neex": "dist/index.js"
  },
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts --minify --sourcemap --clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "nextjs",
    "cli",
    "shadcn-ui",
    "prisma",
    "authentication",
    "zustand",
    "boilerplate",
    "starter-template",
    "fullstack nextjs",
    "neex"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "@types/cross-spawn": "^6.0.6",
    "commander": "^13.1.0",
    "picocolors": "^1.1.1",
    "cross-spawn": "^7.0.3",
    "log-update": "^6.1.0",
    "gradient-string": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
